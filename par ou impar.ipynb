    "import random\n",
    "\n",
    "jogador = int(input(\"Digite um número: \"))\n",
    "computador = random.randint(0, 10)\n",
    "\n",
    "total = jogador + computador\n",
    "\n",
    "print(f\"Você jogou {jogador}, o computador jogou {computador}. Total: {total}\")\n",
    "\n",
    "if total % 2 == 0:\n",
    "    print(\"Deu PAR!\")\n",
    "else:\n",
    "    print(\"Deu ÍMPAR!\")"
   ]
  }
